# Local development environment variables
# Copy this to .env.local and update with your actual credentials

# Qdrant Vector Database Configuration
QDRANT_URL=https://your-cluster-id.us-east4-0.gcp.cloud.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key

# Cohere API Key
COHERE_API_KEY=your-cohere-api-key
COHERE_MODEL=command-r-plus

# Neon Postgres Database Configuration
DATABASE_URL=postgresql://user:password@host:5432/physical_ai?sslmode=require

# Backend URL for chatbot communication
REACT_APP_BACKEND_URL=http://localhost:8000
